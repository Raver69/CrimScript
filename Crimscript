local mt = getrawmetatable(game)
local oldIndex = mt.__index
local oldNamecall = mt.__namecall

setreadonly(mt,false)

mt.__namecall = newcclosure(function(tab,...)
    local args = {...}
    if(_G.noFall and getnamecallmethod()=="FireServer" and tab.Name=="__DFfDD")then
        return
    end
    return oldNamecall(tab, unpack(args))
end)

mt.__index = newcclosure(function(tab,key)

	if(checkcaller() and key =="isSelected")then
		return (oldIndex(tab,"BackgroundColor3") == Color3.new(0,1,0))
	end

	return oldIndex(tab,key)    
end)

