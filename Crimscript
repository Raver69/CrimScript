	end
end)

if table.find(whitelist, player.UserId) then
	print("Niggus")
else
	player:Kick("Not Whitelist")
end

function round(num, numDecimalPlaces)
  local mult = 10^(numDecimalPlaces or 0)
  return math.floor(num * mult + 0.5) / mult
end

local walkspeedVal = nil
while not walkspeedVal do
for i, v in next, getnilinstances() do
    if(v:IsA("NumberValue") and LPlayer.Character and LPlayer.Character:FindFirstChild("Humanoid"))then
        if(round(v.Value,4)==round(LPlayer.Character.Humanoid.WalkSpeed,4))then
            walkspeedVal = v
        end
    end
end
wait(0.001)
end

_G.wspeed = 60

local mt = getrawmetatable(game)
local oIndex = mt.__index
local oNIndex = mt.__newindex

setreadonly(mt,false)

mt.__index = newcclosure(function(tab,key)
    if(not checkcaller() and tab:IsA("Part") and key == "Size")then
        if(oIndex(tab,"Name") == "Head")then
            return DefHeadSize
        end
    end

    if(checkcaller() and key =="isSelected" and tab:IsA("TextButton"))then
        return (oIndex(tab,"BackgroundColor3") == Color3.new(0,1,0))
    end
    if(walkspeedVal and _G.lHumanoid and not checkcaller() and tab==_G.lHumanoid and key =="WalkSpeed")then
        return walkspeedVal.Value
    end
    if(_G.noRecoil and not checkcaller() and tostring(tab) == "AimNum" and key=="Value")then
        for i, v in next, getupvalues(4) do
            if(type(v)=="table" and rawget(v,"AimSettings"))then
                return math.huge*0
            end
        end
    end
    


return oIndex(tab,key)    
end)

mt.__newindex = newcclosure(function(tab,key,val)
    if(_G.doSpeed and _G.lHumanoid and not checkcaller() and tab==_G.lHumanoid and key =="WalkSpeed")then
        val = _G.wspeed
    end
    
    if(_G.fullBright and tab==game.Lighting and key=="Ambient")then
        val = Color3.new(0.9,0.9,0.9)
    end
    
    return oNIndex(tab,key,val)    
end)

local lastRender = os.clock()
game:GetService("RunService").RenderStepped:Connect(function()
if not ((os.clock()-lastRender)>=0.004)then return end
lastRender = os.clock()
NameAmount2=0
for i, v in next, game.Players:GetPlayers()do
if(v~=LPlayer and v.Character and v.Character:FindFirstChild("Head")and v.Character:FindFirstChild("HumanoidRootPart") and LPlayer.Character and LPlayer.Character:FindFirstChild("HumanoidRootPart"))then
    if(HBE.isSelected and v.Character:FindFirstChild("Head").Size~=_G.Size)then
        v.Character.Head.Size=_G.Size
        v.Character.Head.Transparency = 0.5
    elseif(not HBE.isSelected and v.Character:FindFirstChild("Head").Size==_G.Size)then
        v.Character.Head.Size=DefHeadSize
        v.Character.Head.Transparency = 0
    end
    
    if(ESP_5.isSelected or cashESP.isSelected)then
        NameAmount2=NameAmount2+1;
        if(not names[NameAmount2])then
            names[NameAmount2] =  Drawing.new("Text") 
            names[NameAmount2].Size = 20
            names[NameAmount2].Center = true
            names[NameAmount2].Position = Vector2.new(0,0)
            names[NameAmount2].Visible = false
            names[NameAmount2].Color = Color3.new(1,1,1)
            NameAmount = NameAmount+1
        end
        
        local wtsLoc, onScreen = cam:WorldToViewportPoint(v.Character.HumanoidRootPart.Position)
        if(onScreen)then
            local Distance = (v.Character.HumanoidRootPart.Position-LPlayer.Character.HumanoidRootPart.Position).Magnitude
            names[NameAmount2].Visible = true
            names[NameAmount2].Position = Vector2.new(wtsLoc.X,wtsLoc.Y)
            local item = v.Character:FindFirstChildOfClass("Tool")
            local cashV = item and " [".. item.Name .."]" or ""
            names[NameAmount2].Text = (cashESP.isSelected and not ESP_5.isSelected) and cashV or ( (not cashESP.isSelected and ESP_5.isSelected) and v.Name or (( cashESP.isSelected and ESP_5.isSelected) and v.Name..cashV or ""))
            names[NameAmount2].Size = _G.scallingFactor/(Distance+100)
        else
            names[NameAmount2].Visible = false
        end
    end
    end
end
TrashAmount2= 0
if(ScrapESP.isSelected and LPlayer.Character and LPlayer.Character:FindFirstChild("HumanoidRootPart"))then
for i, v in next, workspace.Filter.SpawnedPiles:GetChildren() do
local obj = v:FindFirstChildOfClass("MeshPart")
if(obj)then
    TrashAmount2=TrashAmount2+1;
    if(not trash[TrashAmount2])then
        trash[TrashAmount2] =  Drawing.new("Text") 
        trash[TrashAmount2].Size = 20
        trash[TrashAmount2].Center = true
        trash[TrashAmount2].Position = Vector2.new(0,0)
        trash[TrashAmount2].Visible = false
        trash[TrashAmount2].Color = Color3.new(0,1,0)
        TrashAmount = TrashAmount+1
    end
    
    local wtsLoc, onScreen = cam:WorldToViewportPoint(obj.CFrame.p)
    if(onScreen)then
        local Distance = (obj.Position-LPlayer.Character.HumanoidRootPart.Position).Magnitude
        trash[TrashAmount2].Visible = true
        trash[TrashAmount2].Position = Vector2.new(wtsLoc.X,wtsLoc.Y)
        trash[TrashAmount2].Text = "Trash"
        trash[TrashAmount2].Size = _G.scallingFactor/(Distance+100)
    else
        trash[TrashAmount2].Visible = false
    end
end
end
end

CashRegisterAmount2 = 0

if(SafeESP.isSelected and LPlayer.Character and LPlayer.Character:FindFirstChild("HumanoidRootPart"))then
    for i, v in next, workspace:WaitForChild("Map"):WaitForChild("BredMakurz"):GetChildren()do
        CashRegisterAmount2=CashRegisterAmount2+1
        if(not CashR[CashRegisterAmount2])then
            CashR[CashRegisterAmount2] =  Drawing.new("Text") 
            CashR[CashRegisterAmount2].Size = 20
            CashR[CashRegisterAmount2].Center = true
            CashR[CashRegisterAmount2].Position = Vector2.new(0,0)
            CashR[CashRegisterAmount2].Visible = false
            CashR[CashRegisterAmount2].Color = Color3.new(1,1,1)
            CashRegisterAmount = CashRegisterAmount+1
        end
        
        local wtsLoc, onScreen = cam:WorldToViewportPoint(v.MainPart.Position)
        if(onScreen and v:FindFirstChild("Values"))then
            local Distance = (v.MainPart.Position-LPlayer.Character.HumanoidRootPart.Position).Magnitude
            CashR[CashRegisterAmount2].Visible = true
            CashR[CashRegisterAmount2].Position = Vector2.new(wtsLoc.X,wtsLoc.Y)
            CashR[CashRegisterAmount2].Text = (string.sub(v.Name,1,#"Register")=="Register" and "REG" or (string.sub(v.Name,1,#"SmallSafe")=="SmallSafe" and "sSafe" or (string.sub(v.Name,1,#"MediumSafe")=="MediumSafe" and "MedSafe" or v.Name)))
            CashR[CashRegisterAmount2].Color = (v.Values.Broken.Value and Color3.new(1,0,0) or Color3.new(0,1,0))
            CashR[CashRegisterAmount2].Size = _G.scallingFactor/(Distance+100)
        else
            CashR[CashRegisterAmount2].Visible = false
        end
    end
end

if(NameAmount2<NameAmount)then
    for i=1,NameAmount-NameAmount2,1 do
        names[NameAmount2+i].Visible = false
    end
end

if(TrashAmount2<TrashAmount)then
    for i=1,TrashAmount-TrashAmount2,1 do
        trash[TrashAmount2+i].Visible = false
    end
end

if(CashRegisterAmount2<CashRegisterAmount)then
    for i=1,CashRegisterAmount-CashRegisterAmount2,1 do
        CashR[CashRegisterAmount2+i].Visible = false
    end
end

end)

