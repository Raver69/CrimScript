    _G.doSpeed = Speed_2.isSelected
    _G.lHumanoid = LPlayer.Character and LPlayer.Character:FindFirstChild("Humanoid") or nil --had to do it like this because if i do it in the hook its laggy asf
    _G.instaPickup = FastPickup_2.isSelected
    _G.infStamina = Infstamina_2.isSelected
    _G.noRecoil = NoRecoil_2.isSelected
    _G.fullBright = Fullbright_3.isSelected
    _G.mainFrame.Visible = ChatLogger.isSelected
    _G.noFall = ComingSoon1_2.isSelected
    
    wait(0.001)
end
	end
end)

