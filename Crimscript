if(_G.infStamina)then
    v1 = 0
end
return oldFunc(v1,...)
end)

local defaultAimNum = {}

local oldBrightness = game.Lighting.Ambient

local camera = workspace.CurrentCamera

local screenGui = Instance.new("ScreenGui")
syn.protect_gui(screenGui)
screenGui.Parent = game.CoreGui

_G.mainFrame = Instance.new("Frame")
_G.mainFrame.Size = UDim2.new(0,600,0,300)
_G.mainFrame.Position = UDim2.new(0,camera.ViewportSize.X-_G.mainFrame.Size.X.Offset,0,camera.ViewportSize.Y-_G.mainFrame.Size.Y.Offset)
_G.mainFrame.BackgroundColor3 = Color3.new(0.1,0.1,0.1)
_G.mainFrame.Transparency = 0.3
_G.mainFrame.Parent = screenGui
_G.mainFrame.Transparency = 0.3
_G.mainFrame.Visible = true
_G.mainFrame.Active = true

_G.moveFrame = Instance.new("Frame")
_G.moveFrame.Parent = _G.mainFrame
_G.moveFrame.Size = UDim2.new(1,0,0.1)
_G.moveFrame.BackgroundColor3 = Color3.new(0.1,0.1,0.1)

_G.textLabels = {}

local function chatted(plr, msg)
    local index = #_G.textLabels+1
    local newPos = (_G.textLabels[index-1] and Vector2.new(_G.textLabels[index-1].Position.X.Offset,_G.textLabels[index-1].Position.Y.Offset+_G.textLabels[index-1].TextBounds.Y) or Vector2.new(5,5)) -- UDim2.new(0,5,0.1,5)
    
    local selectionFrame = Instance.new("Frame")
    selectionFrame.BorderSizePixel =0
    selectionFrame.BackgroundColor3 = Color3.new()
    selectionFrame.BackgroundTransparency =1
    selectionFrame.Visible = true
    
    _G.textLabels[index] = Instance.new("TextLabel")
    _G.textLabels[index].Position = UDim2.new(0,newPos.X,0.1,newPos.Y)
    _G.textLabels[index].Text = "["..plr.Name.."]: "..msg
    _G.textLabels[index].Parent = _G.mainFrame
    _G.textLabels[index].TextXAlignment = Enum.TextXAlignment.Left
    _G.textLabels[index].TextYAlignment = Enum.TextYAlignment.Top
    _G.textLabels[index].TextColor3 = Color3.new(1,1,1)
    _G.textLabels[index].BackgroundTransparency = 1
    _G.textLabels[index].TextSize = 10
    _G.textLabels[index].TextWrapped = true
    _G.textLabels[index].BorderSizePixel =0
    _G.textLabels[index].BackgroundColor3 = Color3.new()
    _G.textLabels[index].Size = UDim2.new(0.9,0,1,0)
    _G.textLabels[index].ZIndex = 10
    
    local relPos = _G.textLabels[index].AbsolutePosition-_G.mainFrame.AbsolutePosition
    selectionFrame.Parent = _G.textLabels[index]
    selectionFrame.Position = UDim2.new(0,-5,0,0)
    selectionFrame.Size = UDim2.new(0,_G.mainFrame.AbsoluteSize.X,0,_G.textLabels[index].TextBounds.Y)
    selectionFrame.ZIndex = 9
    
    selectionFrame.MouseEnter:Connect(function()
        
        local TInfo = TweenInfo.new(0.5,Enum.EasingStyle.Linear,Enum.EasingDirection.Out)
        local Tween = TS:Create(selectionFrame,TInfo,{BackgroundTransparency = 0.5})
        Tween:Play()
    end)
    
    selectionFrame.MouseLeave:Connect(function()
        
        local TInfo = TweenInfo.new(0.5,Enum.EasingStyle.Linear,Enum.EasingDirection.Out)
        local Tween = TS:Create(selectionFrame,TInfo,{BackgroundTransparency = 1})
        Tween:Play()
    end)
    
    local posInMenu = (_G.textLabels[index].AbsolutePosition + _G.textLabels[index].TextBounds) -_G.mainFrame.AbsolutePosition
    if(posInMenu.Y>_G.mainFrame.AbsoluteSize.Y)then
    local startPos = Vector2.new(5,5)
    local nIndex = 1
        for i =1, #_G.textLabels do
            if (posInMenu.Y>=_G.mainFrame.AbsoluteSize.Y) then
                posInMenu = Vector2.new(posInMenu.X, posInMenu.Y - _G.textLabels[i].TextBounds.Y)
                _G.textLabels[i]:Destroy()
            else
                _G.textLabels[i].Position = UDim2.new(0,startPos.X,0.1,startPos.Y)
                startPos = startPos+Vector2.new(0,_G.textLabels[i].TextBounds.Y)
                _G.textLabels[nIndex] = _G.textLabels[i]
                _G.textLabels[i] = nil
                nIndex=nIndex+1
            end
        end
    end
end

for i, v in next, game.Players:GetPlayers() do
    v.Chatted:Connect(function(msg)
        chatted(v,msg)
    end)
end

game.Players.ChildAdded:Connect(function(c)
c.Chatted:Connect(function(msg)
    chatted(c,msg)
end)    
end)

local isDown = false
local LastPoint = Vector2.new()
local LastOffset = Vector2.new()
_G.moveFrame.InputBegan:Connect(function(vInput)
    if(vInput.UserInputType==Enum.UserInputType.MouseButton1)then
        LastPoint = Vector2.new(LocalMouse.X,LocalMouse.Y)
        LastOffset = Vector2.new(_G.mainFrame.Position.X.Offset,_G.mainFrame.Position.Y.Offset)
        isDown = true
    end
end)
inputService.InputChanged:Connect(function(vInput)
    if(vInput.UserInputType == Enum.UserInputType.MouseMovement and isDown)then
        _G.mainFrame.Position = UDim2.new(0,LastOffset.X-(LastPoint.X-LocalMouse.X),0,LastOffset.Y-(LastPoint.Y-LocalMouse.Y))
    end
end)
_G.moveFrame.InputEnded:Connect(function(vInput)
    if(vInput.UserInputType==Enum.UserInputType.MouseButton1)then
        isDown = false
    end
end)

local dropCheck
for i, v in next, getgc() do
    if(type(v)=="function" and getinfo(v).name=="ToolDropCheck")then
        dropCheck = v; break    
    end
end
local cDropCheck
for i, v in next, getgc() do
    if(type(v)=="function" and getinfo(v).name=="CashDropCheck")then
        cDropCheck = v; break    
    end
end

local oldDropCheck
oldDropCheck = hookfunction(dropCheck,function()
    local outP = {oldDropCheck()}
    if(_G.instaPickup and type(outP[1])=="table")then
        outP[1][1][2] = 0
    end
    return unpack(outP)
end)
local oldcDropCheck
oldcDropCheck = hookfunction(cDropCheck,function()
    local outP = {oldcDropCheck()}
    if(_G.instaPickup and type(outP[1])=="table")then
        outP[1][1][2] = 0
    end
    return unpack(outP)
end)

local rcFunction
while not rcFunction do
for i, v in next, getgc(true) do
    if(type(v)=="table" and type(rawget(v,"lol"))=="function" and getinfo(v.lol).source:find(".RCHB"))then
        rcFunction = v.lol
        
    end
end
wait(0.01)
end

local grcFunction
while not grcFunction do
for i, v in next, getgc(true) do
    if(type(v)=="table" and type(rawget(v,"OnRayHit_Sub2"))=="function")then
        grcFunction = v.OnRayHit_Sub2
        print("found")
    end
end
wait(0.01)
end

local numberT = math.huge
for i, v in next, getconstants(rcFunction) do
    if(v=="Head" and type(getconstant(rcFunction,i+1))=="number" and getconstant(rcFunction,i+2)=="Left Arm")then
        setconstant(rcFunction,i+1,numberT)
    end
    if(v==10)then
        setconstant(rcFunction,i,numberT) 
    end
end

for i, v in next, getconstants(grcFunction) do
    if(v=="Head" and type(getconstant(grcFunction,i+1))=="number" and getconstant(grcFunction,i+2)=="Left Arm")then
        setconstant(grcFunction,i+1,numberT)
    end
    if(v==10)then
        setconstant(grcFunction,i,numberT) 
    end
end

inputService.InputBegan:Connect(function(inp)
    if(inp.KeyCode == Enum.KeyCode[_G.HBEKey])then
        HBE.BackgroundColor3 = HBE.BackgroundColor3 == Color3.new(1,0,0) and Color3.new(0,1,0) or Color3.new(1,0,0)
    end
end)

